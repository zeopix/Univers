{% extends 'IgaNewsBundle::layout.html.twig' %}
{% block content %}
<div id="results">
<h1>Category creation</h1>
<br>
<form action="{{ path('categories_create') }}" method="post" {{ form_enctype(form) }}>
    {{ form_widget(form) }}
    <p>
        <button type="submit">Create</button>
    </p>
</form>

<ul class="record_actions">
    <li>
        <a href="{{ path('categories') }}">
            Back to the list
        </a>
    </li>
</ul>
</div>
{% endblock %}